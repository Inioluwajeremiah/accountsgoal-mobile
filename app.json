{
  "expo": {
    "name": "accountsgoal-app",
    "slug": "accountsgoal-app",
    "scheme": "accountsgoal",
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "icon": "./assets/adaptive-icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "config": {
        "googleMapsApiKey": "AIzaSyBx5KGKK1oPjtvLbiumwQ8DS-a9GpSVLWI"
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.inioluwajeremiah.accountsgoalapp"
    },
    "android": {
      "congig": {
        "googleMaps": {
          "apiKey": "AIzaSyBx5KGKK1oPjtvLbiumwQ8DS-a9GpSVLWI"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.inioluwajeremiah.accountsgoalapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "8717ccc5-70eb-4722-b87f-559087a04ad0"
      }
    }
  }
}
